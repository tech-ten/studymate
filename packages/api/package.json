    "@aws-sdk/client-dynamodb": "^3.700.0",
    "@aws-sdk/lib-dynamodb": "^3.700.0",
    "groq-sdk": "^0.8.0",
    "stripe": "^20.1.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.147",
    "@types/node": "^22.10.2",
    "@types/uuid": "^11.0.0",
    "typescript": "^5.7.2"
  }
}
